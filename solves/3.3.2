import torch
import torch.nn as nn

x = torch.rand(1, 16, 16) # тензор x в программе не менять

model = nn.Sequential(
    nn.Conv2d(1, 32, 3, padding=1),
    nn.ReLU(inplace=True),
    nn.MaxPool2d(2),
    nn.Conv2d(32, 16, 3, padding=1),
    nn.ReLU(inplace=True),
    nn.MaxPool2d(2),
    nn.Flatten(),
    nn.Linear(256, 5)
)

predict = model(x.unsqueeze(0))
